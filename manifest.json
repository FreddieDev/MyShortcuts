{
	"name":"MyShortcuts",
	"description":"Quick access to Capgemini websites and tools from Chrome.",
	"version":"1.0",
	"icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    },
	
	"manifest_version":2,
	"content_security_policy": "default-src 'none'; script-src 'self'; connect-src 'self'",
	
	"permissions":[
		"storage"
	],
	"options_ui":{
		"page":"options.html",
		"open_in_tab":false
	},
	"browser_action":{
		"default_popup":"options.html"
	}
}